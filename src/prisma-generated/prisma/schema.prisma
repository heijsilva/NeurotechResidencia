generator client {
  provider = "prisma-client-js"
  output   = "../src/prisma-generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model users {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String @unique
  name  String
  age   String
  pets  pets[] @relation("UserPets")
}

model pets {
  id     String @id @default(auto()) @map("_id") @db.ObjectId
  name   String
  age    String
  userId String @db.ObjectId
  user   users  @relation("UserPets", fields: [userId], references: [id])
}
